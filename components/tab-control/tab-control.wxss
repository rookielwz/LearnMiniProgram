/* components/tab-control/tab-control.wxss */
.tab-control {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  justify-content: space-between;
  font-size: 30rpx;
}
.titleItem {
  flex: 1;
  text-align: center;
}
.active {
  color: #ff5777;
  padding: 10rpx 8rpx;
  border-bottom: 6rpx solid #ff5777;
}